                     version="1.0.0">
    <CoverageOffering>
        <name>demo.conc_chl</name>
        <label>Chlorophyll concentration</label>
        <lonLatEnvelope>
            <gml:pos>0 50</gml:pos>
            <gml:pos>5 52.5</gml:pos>
        </lonLatEnvelope>
        <domainSet>
            <spatialDomain>
                <gml:Envelope>
                    <gml:pos>0 50</gml:pos>
                    <gml:pos>5 52.5</gml:pos>
                </gml:Envelope>
            </spatialDomain>
        </domainSet>
        <rangeSet>
            <RangeSet>
                <name>demo.conc_chl</name>
                <label>Chlorophyll concentration</label>
                <axisDescription>
                    <AxisDescription>
                        <name>Band</name>
                        <label>Band</label>
                        <values>
                            <interval>
                                <min>0.0001</min>
                                <max>22.4421</max>
                            </interval>
                        </values>
                    </AxisDescription>
                </axisDescription>
            </RangeSet>
        </rangeSet>
        <supportedCRSs>
            <requestResponseCRSs>EPSG:4326 EPSG:3857</requestResponseCRSs>
        </supportedCRSs>
        <supportedFormats>
            <formats>zarr</formats>
            <formats>netcdf4</formats>
            <formats>csv</formats>
        </supportedFormats>
    </CoverageOffering>
